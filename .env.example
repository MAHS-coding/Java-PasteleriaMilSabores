# Copy this file to `.env` and fill the values before running docker compose.
# Do NOT commit a real .env with secrets to version control.

# Database credentials for XAMPP/MySQL on the host machine
DB_USER=root
DB_PASS=

# JWT secret (use a long random value, keep secret)
JWT_SECRET=ChangeThisSecretToAStrongRandomValue

# Optional: set registry prefix to push images
REGISTRY=
